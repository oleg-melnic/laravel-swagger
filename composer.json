{
  "name": "oleg-melnic/laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Melnic",
      "email": "oleg.melnic@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": ">=5.3.0",
    "minime/annotations": "2.3.1"
  },
  "autoload": {
    "psr-4": {
      "oleg-melnic\\Support\\AutoDoc\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  },
  "minimum-stability": "dev"
}
